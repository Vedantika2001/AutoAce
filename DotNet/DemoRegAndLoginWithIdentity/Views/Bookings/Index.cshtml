@model IEnumerable<DemoRegAndLoginWithIdentity.Models.Booking>

@{
    ViewData["Title"] = "Index";
}

<h1>View Booking</h1>

<p>
    <a asp-action="Create">New Registration</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustomerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MobileNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VehicleName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VehicleRegistrationNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceCenter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceDesc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VehicleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VehicleRegistrationNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceCenter)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceDesc)
            </td>
            <td>
 
                 <a asp-action="Edit" asp-route-id="@item.Id" class="btn-outline-success">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id" class="btn-check">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn-danger">Delete</a>
                    
            </td>
        </tr>
}
    </tbody>
</table>
